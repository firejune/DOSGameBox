{
  "name": "DOSGameBox",
  "version": "0.6.1",
  "description": "JavaScript Based DOS Games Emulator",
  "license": "MIT",
  "homepage": "https://github.com/firejune/DOSGameBox",
  "main": "main.js",
  "bugs": {
    "url": "https://github.com/firejune/DOSGameBox/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firejune/DOSGameBox.git"
  },
  "dependencies": {
    "configstore": "*",
    "ini": "^1.3.4",
    "jquery": "2.x",
    "jquery.scrollto": "^2.1.2",
    "jszip": "^2.5.0",
    "ncp": "^2.0.0",
    "ws": "*"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.37.2",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-react": "^4.2.1",
    "eslint-plugin-standard": "^1.3.2"
  },
  "string": "--version-string.CompanyName=FIREJUNE --version-string.ProductName=DOSGameBox",
  "ignore": "--ignore=/v86/ --ignore=/build/ --ignore=/bin/misc/",
  "option": "--asar=true --prune --version=0.37.2 --icon=assets/icon.icns --out=build --overwrite",
  "scripts": {
    "start": "electron . development",
    "lint": "eslint src",
    "build": "npm run build-darwin-x64",
    "build-darwin-x64": "electron-packager . --platform=darwin --arch=x64 $npm_package_name $npm_package_ignore $npm_package_string $npm_package_option",
    "build-win32-x64": "electron-packager . --platform=win32 --arch=x64 $npm_package_name $npm_package_ignore $npm_package_string $npm_package_option",
    "build-darwin-x32": "electron-packager . --platform=darwin --arch=x32 $npm_package_name $npm_package_ignore $npm_package_string $npm_package_option",
    "build-win32-x32": "electron-packager . --platform=win32 --arch=x32 $npm_package_name $npm_package_ignore $npm_package_string $npm_package_option"
  }
}
